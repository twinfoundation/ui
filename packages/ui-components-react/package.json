{
	"name": "@twin.org/ui-components-react",
	"version": "0.0.1-next.26",
	"description": "UI components for react",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/twinfoundation/ui.git",
		"directory": "packages/ui-components-react"
	},
	"author": "martyn.janes@iota.org",
	"license": "Apache-2.0",
	"type": "module",
	"engines": {
		"node": ">=20.0.0"
	},
	"scripts": {
		"clean": "rimraf dist",
		"build": "tsc",
		"bundle:esm": "rollup --config rollup.config.mjs --environment MODULE:esm",
		"bundle:cjs": "rollup --config rollup.config.mjs --environment MODULE:cjs",
		"bundle": "npm run bundle:esm && npm run bundle:cjs",
		"dist": "npm run clean && npm run build && npm run bundle"
	},
	"dependencies": {
		"@twin.org/ui-tailwind": "0.0.1-next.26",
		"prop-types": "^15.8.1",
		"react-icons": "^5.4.0"
	},
	"devDependencies": {
		"@types/react": "^18.3.12",
		"@types/react-dom": "^18.3.1",
		"autoprefixer": "^10.4.20",
		"postcss": "^8.4.49",
		"rimraf": "^6.0.1",
		"rollup": "^4.27.3",
		"rollup-plugin-copy": "^3.5.0",
		"tailwindcss": "^3.4.15",
		"typescript": "^5.6.3"
	},
	"peerDependencies": {
		"flowbite": "^2.5.2",
		"flowbite-react": "^0.10.2",
		"react": ">=18",
		"react-dom": ">=18",
		"tailwindcss": "3"
	},
	"exports": {
		".": {
			"require": "./dist/cjs/index.cjs",
			"import": "./dist/esm/index.mjs",
			"types": "./dist/types/index.d.ts"
		},
		"./css/*.css": "./dist/css/*.css",
		"./config/*.mjs": "./dist/config/*.js"
	},
	"files": [
		"dist",
		"docs",
		"locales"
	]
}
